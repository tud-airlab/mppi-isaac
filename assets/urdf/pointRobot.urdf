<?xml version="1.0"?>
<robot name="omni_base">

<link name="plane">
</link>

<link name="link_x">
</link>

<link name="link_y">
<inertial>
  <mass value="10.0"/>
  <origin rpy="0 0 0" xyz="0 0 0"/>
  <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
</inertial>
<collision>
  <origin rpy="0.0 0 0" xyz="0 0 0"/>
  <geometry>
      <cylinder length="0.1" radius="0.2"/>
  </geometry>
</collision>
<visual>
  <origin rpy="0.0 0 0" xyz="0 0 0"/>
  <geometry>
      <cylinder length="0.1" radius="0.2"/>
  </geometry>
</visual>
</link>

<joint name="plane_to_link_x" type="prismatic">
<axis xyz="1 0 0"/>
<origin xyz="0 0 0"/>
<parent link="plane"/>
<child link="link_x"/>
<limit effort="1000.0" velocity="100"/>
</joint>

<joint name="plane_to_link_y" type="prismatic">
<axis xyz="0 1 0"/>
<origin xyz="0 0 0"/>
<parent link="link_x"/>
<child link="link_y"/>
<limit effort="1000.0" velocity="100"/>
</joint>

  
  
</robot>
